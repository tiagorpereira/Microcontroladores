#include <stdio.h>
#include <msp430fr2433.h>

#define LED BIT1

int main(void)
{
    WDTCTL = WDTPW + WDTHOLD;   // Stop WDT

    CSCTL1 = 0xF1;      //MCLK e SMCLK @ 1MHz
    CSCTL0 = 0x511;       //MCLK e SMCLK @ 1MHz
    P1OUT &= ~LED;
    P1DIR |= LED;
    TA0CCR0 = 62500-1; //10000-1;
    TA0CTL = TASSEL_2 + ID_3 + MC_1;
    while(1)
    {
        while((TA0CTL & TA0IFG)==0);
        P1OUT ^= LED;
        TA0CTL &= ~TA0IFG;
    }
    return 0;
}
